# More information at
# https://editorconfig.org/

root = true

[*]
# Windows-style newline ending
end_of_line = crlf 
tab_width = 4
indent_size = 4

[*.md]
charset      = utf-8-bom

[*.cs]
charset      = utf-8-bom
indent_style = space
indent_size  = 4

[*.csproj]
charset      = utf-8-bom
indent_style = space
tab_width	 = 2
indent_size  = 2

[*.gd]
charset      = utf-8-bom
indent_style = space
tab_width	 = 4
indent_size  = 4

[*.nav]
charset      = utf-8-bom
indent_style = space
tab_width	 = 4
indent_size  = 4

[*.sql]
charset      = utf-8-bom
indent_style = space
tab_width	 = 4
indent_size  = 4

[*.stg]
charset      = utf-8-bom
indent_style = space
tab_width	 = 4
indent_size  = 4

[*.props]
charset      = utf-8-bom
indent_style = space
tab_width	 = 2
indent_size  = 2

[*.targets]
charset      = utf-8-bom
indent_style = space
tab_width	 = 2
indent_size  = 2