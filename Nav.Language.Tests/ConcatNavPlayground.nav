task C 
{
    init I1;
    init I2;
    exit e1;
    exit e2;
    I1 --> e1;
    I2 --> e2;
}

task A
{
    view Gui;
    init;  
    choice Choice;
    task C Login; // disable Nav1s012 => Kein Nav1012 Warning mehr
    task C L2; // disable Nav1s012 => Kein Nav1012 Warning mehr

    init            --> Login;

    init            --> Login o-^ Gui if "Foo";   // Init Transition    modal Concat
    Gui             --> Gui o-^ Login on OnF12;   // Trigger Transition modal Concat
    Choice          --> Gui o-^ Login;            // Choice Transition  modal Concat
    Login:e1        --> Gui --^ Login;            // Exit Transition    non-modal Concat

    Login:e2        --> Choice;
}
