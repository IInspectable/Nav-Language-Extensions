task Kalkulation	[base StandardWFS<ArtikelBearbeitenTaskState> :IWFServiceBase]
					[params IArtikelBearbeitenPresenter presenter, APlusVAuswahlDispatcherPresenter dispatcherPresenter, 
							IArtikelRepositoryBS artikelRepositoryBS, ITaskResultFactory taskResultFactory]
					[result ITaskResultWithTO<ArtikelKalkulationErgebnis>]
{

	init	Kalkulation  [params BORef selektierterEintragOid, bool preisgestaltungsTabSelected];
	init	KalkulationFromArtikelsuche  [params BORef selektierterEintragOid];

	task	APlusVAuswahl							    AktionArtikelstammPlusV;	
	task	EtikettenDruckAuswaehlen					DruckAuswahl;
	task	MessageBoxOk								MeldungKeinTrefferGefunden;
	choice	OnF9Choice;
	choice	Preisgestaltung_HatKeinenEigenenVK;
	choice	Preisgestaltung_SindPreisberechnungsangabenGueltig;
	choice	WurdeTrefferFuerAplusVArtikelGefunden;                   

	/*
	task	Artikelsuche;
	task	MessageBoxOk	MsgKeinAlternativartikel;
	task	MessageBoxOk	MsgZuVieleAlternativartikel;
	task	MessageBoxOk	MsgArtikelKannNichtUebernommenWerden;
	
	choice CheckAnzahlAlternativartikel;
	choice CheckGruppennummerDesUebernehmendenArtikels;
	*/

	exit						Return;


	// -------------------------------------------------------------------------------------------------        
    // F9
    // -------------------------------------------------------------------------------------------------    
    Kalkulation										--> OnF9Choice;
    KalkulationFromArtikelsuche						--> OnF9Choice;
    OnF9Choice									    --> AktionArtikelstammPlusV if "A+V Artikel";
    OnF9Choice										--> DruckAuswahl if "Druck der Etiketten";
    OnF9Choice										--> Return;

    DruckAuswahl:Done								--> Return;
    AktionArtikelstammPlusV:Durchgefuehrt			--> Return if "Abgebrochen";
    AktionArtikelstammPlusV:Abgebrochen			    --> WurdeTrefferFuerAplusVArtikelGefunden;
    AktionArtikelstammPlusV:Exit_Artikelsuchen		--> Return if "Abgebrochen";
	/*
	AktionArtikelstammPlusV:Exit_Artikelsuchen		--> CheckAnzahlAlternativartikel if "indirekt; via F12-OK in Alternativ Artikel" do "Artikelsuche öffnen";
	
	CheckAnzahlAlternativartikel		o-> MsgKeinAlternativartikel if "Keinen Alternativartikel gefunden";
	CheckAnzahlAlternativartikel		o-> MsgZuVieleAlternativartikel if "Zu viele Alternativartikel gefunden";
	CheckAnzahlAlternativartikel		o-> Artikelsuche if "alles OK";

	MsgKeinAlternativartikel:Ok			--> AktionArtikelstammPlusV do "Artikel nicht ersetzen";
	MsgZuVieleAlternativartikel:Ok		o-> Artikelsuche if "Erste 500 Treffer anzeigen";

	Artikelsuche:ArtikelAusgewaehlt	--> CheckGruppennummerDesUebernehmendenArtikels if "Artikel ausgewählt" do "Check Artikel hat der gleichen Gruppennummer";
	Artikelsuche:Abbruch			--> AktionArtikelstammPlusV do "Artikel nicht ersetzen";

	CheckGruppennummerDesUebernehmendenArtikels	--> AktionArtikelstammPlusV if "Artikel mit gleicher Gruppennummer" do "Artikel ersetzen";
	CheckGruppennummerDesUebernehmendenArtikels	o-> MsgArtikelKannNichtUebernommenWerden if "Artikel mit unterschiedlicher Gruppennummer" do "Error-Meldung anzeigen";

	MsgArtikelKannNichtUebernommenWerden:Ok	--> AktionArtikelstammPlusV do "Artikel nicht ersetzen";
	*/

	WurdeTrefferFuerAplusVArtikelGefunden			--> MeldungKeinTrefferGefunden if "Kein Treffer gefunden";
	WurdeTrefferFuerAplusVArtikelGefunden			--> Return if "Treffer gefunden";

	MeldungKeinTrefferGefunden:Ok					--> Return;
}


