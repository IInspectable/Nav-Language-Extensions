<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
 
  <UsingTask TaskName="Pharmatechnik.Nav.Language.BuildTasks.NavCodeGenerator" AssemblyFile="Pharmatechnik.Nav.Language.BuildTasks.dll" />

  <ItemGroup>
    <AvailableItemName Include="NavFile"/>
  </ItemGroup>

  <PropertyGroup>
    <GenerateNavCodeForce Condition="'$(GenerateNavCodeForce)' == ''">false</GenerateNavCodeForce>
    <GenerateNavCodeGenerateToClasses Condition="'$(GenerateNavCodeGenerateToClasses)' == ''">true</GenerateNavCodeGenerateToClasses>
    <GenerateNavCodeUseSyntaxCache Condition="'$(GenerateNavCodeUseSyntaxCache)' == ''">false</GenerateNavCodeUseSyntaxCache>
  </PropertyGroup>

  <Target Name="GenerateNavCode" BeforeTargets="PrepareForBuild">
    <NavCodeGenerator Files="@(NavFile)"
                      Force="$(GenerateNavCodeForce)"
                      GenerateToClasses="$(GenerateNavCodeGenerateToClasses)"
                      UseSyntaxCache="$(GenerateNavCodeUseSyntaxCache)" />
  </Target>
  
</Project>